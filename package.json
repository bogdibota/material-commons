    "@material-ui/icons": "^4.2.1",
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.2.0",
    "react-scripts-ts": "3.1.0",
    "rollup": "^0.62.0",
